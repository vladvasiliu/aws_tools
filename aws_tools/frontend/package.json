    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "vue"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
